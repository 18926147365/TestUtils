<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.FundTalkConfMapper">
    <resultMap id="BaseResultMap" type="bean.FundTalkConf">
        <result column="id" property="id"></result>
        <result column="access_token" property="accessToken"></result>
        <result column="fund_id" property="fundId"></result>
        <result column="state" property="state"></result>
        <result column="create_time" property="createTime"></result>

    </resultMap>


    <select id="queryAll" resultMap="BaseResultMap">
        select * from fund_talk_conf where state = 0
    </select>
</mapper>